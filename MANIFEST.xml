<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project CS211-Final-Project with Jar-in-Jar Loader">
    <!--this file was created by Eclipse Runnable JAR file Export Wizard-->
    <!--ANT 1.7 is required-->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="/Users/janneyfamily/eclipse-workspace"/>
    <property name="dir.jarfile" value="/Users/janneyfamily/git"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/JavaMaker.jar">
            <manifest>
                <attribute name="Main-Class" value="org.eclipse.jdt.internal.jarinjarloader.JarRsrcLoader"/>
                <attribute name="Rsrc-Main-Class" value="com.javamaker.App"/>
                <attribute name="Class-Path" value="."/>
                <attribute name="Rsrc-Class-Path" value="./ javafx-swt.jar javafx.base.jar javafx.controls.jar javafx.fxml.jar javafx.graphics.jar javafx.media.jar javafx.swing.jar javafx.web.jar"/>
            </manifest>
            <zipfileset src="jar-in-jar-loader.zip"/>
            <fileset dir="${dir.buildfile}/bin"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx-swt.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.base.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.controls.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.fxml.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.graphics.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.media.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.swing.jar"/>
            <zipfileset dir="/Users/janneyfamily/Downloads/JavaFx/lib" includes="javafx.web.jar"/>
        </jar>
    </target>
</project>
